{
  "python.languageServer": "Pylance",
  "python.analysis.extraPaths": ["../app0-admin/app0-admin/src", "../app0-admin/plugins/platform-auth/src"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.enabled": true,
  "isort.args": ["--src=${workspaceFolder}"],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.mypy_cache": true,
    "**/venv": true,
    "**/*.egg-info": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.mypy_cache": true,
    "**/venv": true,
    "**/*.egg-info": true,
  }
}
